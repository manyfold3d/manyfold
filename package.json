{
  "name": "manyfold",
  "private": true,
  "scripts": {
    "lint:ts": "ts-standard --fix",
    "build": "bundle exec i18n export --config=./config/i18n-js.yml && esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
    "build:css": "sass ./app/assets/stylesheets/application.bootstrap.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules",
    "typecheck": "bundle exec i18n export --config=./config/i18n-js.yml && tsc --project tsconfig.json"
  },
  "dependencies": {
    "@nathanvda/cocoon": "^1.2.14",
    "@popperjs/core": "^2.11.8",
    "@rails/ujs": "^7.1.3",
    "@selectize/selectize": "^0.15.2",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "comlink": "^4.4.1",
    "esbuild": "^0.21.3",
    "i18n-js": "^4.4.3",
    "jquery": "^3.7.1",
    "masonry-layout": "^4.2.2",
    "sass": "^1.77.2",
    "three": "^0.164.1",
    "threejs-webworker-3mf-loader": "https://github.com/manyfold3d/threejs-webworker-3mf-loader"
  },
  "version": "0.1.0",
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/eslint-parser": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@types/bootstrap": "^5.2.10",
    "@types/i18n-js": "^3.8.9",
    "@types/jquery": "^3.5.30",
    "@types/node": "^20.11.0",
    "@types/selectize": "^0.12.39",
    "@types/three": "^0.164.0",
    "@types/webpack-env": "^1.18.5",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "ts-standard": "^12.0.2",
    "typescript": "^5.4.5"
  }
}
