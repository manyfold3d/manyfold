{
  "name": "manyfold",
  "private": true,
  "scripts": {
    "lint:ts": "ts-standard --fix",
    "build": "bundle exec i18n export --config=./config/i18n-js.yml && esbuild app/javascript/*.* --minify=true --tree-shaking=true --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
    "build:css": "sass ./app/assets/stylesheets/application.bootstrap.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules",
    "typecheck": "bundle exec i18n export --config=./config/i18n-js.yml && tsc --project tsconfig.json"
  },
  "dependencies": {
    "@notus.sh/cocooned": "^2.3.0",
    "@popperjs/core": "^2.11.8",
    "@rails/ujs": "^7.1.400",
    "@selectize/selectize": "^0.15.2",
    "@uppy/core": "^3.13.1",
    "@uppy/dashboard": "^3.9.1",
    "@uppy/form": "^3.2.2",
    "@uppy/locales": "^3.5.4",
    "@uppy/xhr-upload": "^3.6.8",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "comlink": "^4.4.1",
    "esbuild": "^0.23.1",
    "i18n-js": "^4.4.3",
    "jquery": "^3.7.1",
    "masonry-layout": "^4.2.2",
    "rpg-awesome": "^0.2.0",
    "sass": "^1.78.0",
    "three": "^0.168.0",
    "threejs-webworker-3mf-loader": "https://github.com/manyfold3d/threejs-webworker-3mf-loader"
  },
  "version": "0.1.0",
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/preset-typescript": "^7.24.7",
    "@types/bootstrap": "^5.2.10",
    "@types/i18n-js": "^3.8.9",
    "@types/jquery": "^3.5.30",
    "@types/node": "^20.16.5",
    "@types/selectize": "^0.12.39",
    "@types/three": "^0.167.1",
    "@types/webpack-env": "^1.18.5",
    "@typescript-eslint/parser": "^8.5.0",
    "eslint": "^8.57.0",
    "husky": "^9.1.6",
    "ts-standard": "^12.0.2",
    "typescript": "^5.6.2"
  }
}
