    "@uppy/core": "^4.2.2",
    "@uppy/dashboard": "^4.1.1",
    "@uppy/form": "^4.0.0",
    "@uppy/locales": "^4.2.0",
    "@uppy/xhr-upload": "^4.2.1",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "comlink": "^4.4.1",
    "esbuild": "^0.24.0",
    "i18n-js": "^4.5.0",
    "jquery": "^3.7.1",
    "masonry-layout": "^4.2.2",
    "rpg-awesome": "^0.2.0",
