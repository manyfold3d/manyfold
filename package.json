    "@types/i18n-js": "^3.8.9",
    "@types/jquery": "^3.5.32",
    "@types/lodash": "^4.17.12",
    "@types/node": "^20.17.1",
    "@types/selectize": "^0.12.39",
    "@types/three": "^0.169.0",
    "@types/webpack-env": "^1.18.5",
    "@types/webxr": "^0.5.20",
    "@typescript-eslint/parser": "^8.12.1",
    "eslint": "^9.13.0",
