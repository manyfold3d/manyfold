{
  "name": "manyfold",
  "private": true,
  "scripts": {
    "lint:ts": "ts-standard --fix",
    "build": "bundle exec i18n export --config=./config/i18n-js.yml && esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
    "build:css": "sass ./app/assets/stylesheets/application.bootstrap.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules",
    "typecheck": "bundle exec i18n export --config=./config/i18n-js.yml && tsc --project tsconfig.json"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.23.3",
    "@nathanvda/cocoon": "^1.2.14",
    "@popperjs/core": "^2.11.8",
    "@rails/ujs": "^7.1.3",
    "@selectize/selectize": "^0.15.2",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.3",
    "esbuild": "^0.20.0",
    "i18n-js": "^4.4.3",
    "jquery": "^3.7.1",
    "masonry-layout": "^4.2.2",
    "popper.js": "^1.16.1",
    "sass": "^1.71.0",
    "sifter": "^0.5.4",
    "three": "^0.161.0",
    "typescript": "^5.3.3",
    "yarn": "^1.22.21"
  },
  "version": "0.1.0",
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.10",
    "@types/i18n-js": "^3.8.9",
    "@types/jquery": "^3.5.29",
    "@types/node": "^18.19.17",
    "@types/selectize": "^0.12.39",
    "@types/three": "^0.161.2",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "husky": "^9.0.11",
    "ts-standard": "^12.0.2"
  }
}
